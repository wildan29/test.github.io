{
  "name": "slim/slim-skeleton",
  "description": "A Slim Framework skeleton application for rapid development",
  "keywords": [
    "microframework",
    "rest",
    "router",
    "psr7"
  ],
  "homepage": "http://github.com/slimphp/Slim-Skeleton",
  "license": "MIT",
  "authors": [
    {
      "name": "Eza Febrian",
      "email": "eza.inet@gmail.com",
      "homepage": "https://eza.lineupdev.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "kreait/firebase-php": "^5.7",
    "monolog/monolog": "^1.17",
    "slim/php-view": "^2.0",
    "slim/slim": "^3.1",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public",
    "test": "phpunit"
  }
}
